<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="FlanaganOranTriviaGame.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:FlanaganOranTriviaGame"
    Shell.FlyoutBehavior="Disabled"
    Title="FlanaganOranTriviaGame">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    
    <ShellContent Title="Cash Builder"
    ContentTemplate="{DataTemplate local:CashBuilder}"/>

    <ShellContent Title="Settings:"
    ContentTemplate="{DataTemplate local:CashBuilder}"/>

</Shell>
